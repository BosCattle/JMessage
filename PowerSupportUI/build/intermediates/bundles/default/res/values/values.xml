<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#000000</color>
    <color name="colorAccent">#FF4081</color>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#303F9F</color>
    <color name="letter_cell">#FBFBFB</color>
    <color name="white">#FFFFFF</color>
    <color name="windowBackground">#FFFF00</color>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="audio_button_redius">5dp</dimen>
    <dimen name="divider">0.2dp</dimen>
    <dimen name="divider_default_height">8dp</dimen>
    <dimen name="divider_normal_height">16dp</dimen>
    <dimen name="text_size_large">20sp</dimen>
    <dimen name="text_size_medium">18sp</dimen>
    <dimen name="text_size_normal">16sp</dimen>
    <string name="app_name">PowerSupportUI</string>
    <string name="audio_cancel">松开手指 取消发送</string>
    <string name="audio_record">按住 说话</string>
    <string name="audio_recording">松开 结束</string>
    <string name="dialog_dismiss_cancel">松开手指 取消发送</string>
    <string name="dialog_too_short">录音时间过短</string>
    <string name="dialog_up_cancel">手指上滑 取消发送</string>
